@model List<Dika.Models.Invertory>

<form method="post" enctype="multipart/form-data" asp-controller="Excel" asp-action="ImportSKU">
    <div class="form-group">
        <label for="file">ფაილი SKU კოდით</label>
        <input type="file" class="form-control-file" id="file" name="exel">
    </div>
    <button type="submit" class="btn btn-primary">Upload</button>
</form>

@if (Model != null)
{
    <table class="table">
        <thead>
            <tr>
                <th>SKU</th>
                <th>Name</th>
                <th>Barcode</th>
                <th>Quantity of Provider</th>
                <th>Size</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.SKU</td>
                    <td>@item.Name</td>
                    <td>@item.Barcode</td>
                    <td>@item.QuantityOfProvider</td>
                    <td>@item.Size</td>
                    <td>@item.Price</td>
                </tr>
            }
        </tbody>
    </table>
}
<form method="post" enctype="multipart/form-data" asp-controller="Excel" asp-action="ImportNoSKU">
    <div class="form-group">
        <label for="file">ფაილი SKU გარეშე</label>
        <input type="file" class="form-control-file" id="file" name="exel">
    </div>
    <button type="submit" class="btn btn-primary">Upload</button>
</form>

@if (Model != null)
{
        <table class="table">
            <thead>
                <tr>
                    <th>SKU</th>
                    <th>Name</th>
                    <th>Barcode</th>
                    <th>Quantity of Provider</th>
                    <th>Size</th>
                    <th>Price</th>
                </tr>
            </thead>
            <tbody>
            @foreach (var item in Model)
            {
                        <tr>
                            <td>@item.SKU</td>
                            <td>@item.Name</td>
                            <td>@item.Barcode</td>
                            <td>@item.QuantityOfProvider</td>
                            <td>@item.Size</td>
                            <td>@item.Price</td>
                        </tr>
            }
            </tbody>
        </table>
}









